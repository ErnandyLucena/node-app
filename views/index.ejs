<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia perguntas</title>
</head>
<body>
    <h1>Guia perguntas!</h1>
    <hr>
    <h3>Lista de perguntas</h3>

   <p>Nome:<%= nome %></p>
   <p>Linguagem:<%= lang %></p>
   <p>Empresa:<%= empresa %></p>
   <p>Inscritos no canal<%= inscritos %></p>

   <% if(msg == true){ %>
    <h3>mensagem de erro</h3>
   <%}
   else{%>
    <h3>nenhum erro</h3>
    <%}%>

    <% produtos.forEach(function(produto) { %>
        <%= produto.nome %>
        <%= produto.preco %>
    <% }) %>


</body>
</html>